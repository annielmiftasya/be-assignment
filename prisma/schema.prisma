
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int      @id @default(autoincrement())
  username String   @unique
  password String
}

model PaymentAccount {
  id        Int      @id @default(autoincrement())
  userId    Int
  type      String
  balance   Float
  createdAt DateTime @default(now())
}
